<UserControl x:Class="LagDaemon.WWB.WWBMain.Controls.MainTabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:cmd="clr-namespace:LagDaemon.MVVM.Commands;assembly=LagDaemon.MVVM.CommandManagement"
             xmlns:local="clr-namespace:LagDaemon.WWB.WWBMain"
             xmlns:controls="clr-namespace:LagDaemon.WWB.WWBMain.Controls"
             xmlns:vm="clr-namespace:LagDaemon.WWB.ViewModels;assembly=LagDaemon.WWB.ViewModels"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid>
        <TabControl>
            <TabItem DataContext="{Binding HomeContext}" Header="Home">
                <controls:HomeTabControl />
            </TabItem>
            <TabItem Header="Synopsis" />
            <TabItem Header="Characters" />
            <TabItem Header="Places" />
            <TabItem Header="Events" />
            <TabItem Header="Time Line" />
            <TabItem Header="Outline" />
            <TabItem Header="Story Structure" />
            <TabItem Header="Settings" />
        </TabControl>

    </Grid>
</UserControl>
